@{
    var lay = "";
    if (Session["LogedUserGrup"] != null)
    {
        var control = Session["LogedUserGrup"].ToString();

        string layout = "";

        if (control == "Supervisor")
        {
            layout = "~/Views/Shared/_LayoutSupervisor.cshtml";
        }
        else if (control == "sales")
        {
            layout = "~/Views/Shared/_LayoutSales.cshtml";
        }

        lay = layout;

    }
    
    Layout = lay;
}